<h1>Add new location</h1>
<%= form_for :location, url: locations_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :lat %><br>
    <%= f.text_field :lat %>
  </p>
  
  <p>
    <%= f.label :long %><br>
    <%= f.text_field :long %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>